<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studyspaces">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".reviews.AllReviews"></activity>
        <activity android:name=".reviews.FullReview" />
        <activity android:name=".space.page.MoreDetails"></activity>
        <activity android:name=".gallery.FullImage" />
        <activity android:name=".gallery.GalleryPage" />
        <activity android:name=".space.StudySpaceItem" />
        <activity
            android:name=".space.search.SearchActivity"
            android:theme="@style/AppTheme.Search" />
        <activity android:name=".ui.login.NewUser" />
        <activity android:name=".users.profile.UserProfile" />
        <activity android:name=".space.page.Page" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".Landing">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>